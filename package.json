{
  "name": "dj-request-app",
  "version": "1.0.0",
  "description": "Real-time DJ song request application with QR code support",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/server.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/server.js",
    "create-dj": "node server/scripts/createDJ.js",
    "create-dj-env": "node server/scripts/createDJFromEnv.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": ["dj", "song-requests", "real-time", "websocket"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
